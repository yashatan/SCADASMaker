<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:DiagramDesigner">
    <s:Toolbox x:Key="BasicControl"
               DefaultItemSize="90,50">
        <Button IsHitTestVisible="False" ToolTip="Button" Background="Blue" MinHeight="25" MinWidth="45" Height="auto">
            <TextBlock Text="Button"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Stretch"
                       ></TextBlock>
        </Button>
        <TextBlock IsHitTestVisible="False" Foreground="Black" Text="Label" FontSize="25" ToolTip="Label" ></TextBlock>
        <TextBox IsHitTestVisible="False"
                 Text="123"
                 FontSize="25"
                 ToolTip="I/O Field"></TextBox>
    </s:Toolbox>
</ResourceDictionary>